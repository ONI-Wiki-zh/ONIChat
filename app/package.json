{
    "name": "oni-chat",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "version": "1.0.0",
    "license": "MIT",
    "repository": "https://github.com/ONI-Wiki-zh/ONIChat",
    "author": "DDElephant",
    "scripts": {
        "start": "cross-env TS_NODE_TRANSPILE_ONLY=true yarn koishi start ./src/koishi.config.ts --watch -- -- -r ts-node/register",
        "yallage": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true yarn koishi start ./src/koishi.config.yallage.ts --watch -- -- -r ts-node/register",
        "yallage-test": "cross-env TS_NODE_TRANSPILE_ONLY=true yarn koishi start ./src/koishi.config.yallage.ts --watch -- -- -r ts-node/register",
        "lint": "eslint . --fix",
        "dev": "ts-node src index.ts --trace-warnings",
        "build": "tsc",
        "run-build": "cross-env NODE_ENV=production node ./dist/index.js",
        "run-relay": "cross-env NODE_ENV=production node ./dist/relayBot.js",
        "test-relay": "cross-env NODE_ENV=production ts-node src/relayBot.ts --trace-warnings",
        "test-build": "node ./dist/index.js"
    },
    "devDependencies": {
        "@koishijs/cli": "^4.0.0-rc.1",
        "@types/jquery": "^3.5.9",
        "@types/node": "^16.11.7",
        "@types/textversionjs": "^1.1.1",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "prettier": "^2.4.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.4.4"
    },
    "dependencies": {
        "@koishijs/plugin-adapter-discord": "^2.0.0-rc.1",
        "@koishijs/plugin-adapter-onebot": "^4.0.0-rc.0",
        "@koishijs/plugin-adapter-telegram": "^2.0.0-rc.0",
        "@koishijs/plugin-admin": "^1.0.0-rc.2",
        "@koishijs/plugin-assets-local": "^2.0.0",
        "@koishijs/plugin-chat": "^1.0.0-rc.1",
        "@koishijs/plugin-common": "^5.0.0-rc.1",
        "@koishijs/plugin-console": "^1.0.0-rc.1",
        "@koishijs/plugin-database-mysql": "^4.0.0-rc.2",
        "@koishijs/plugin-github": "^4.0.0-rc.0",
        "@koishijs/plugin-manager": "^1.0.0-rc.1",
        "@koishijs/plugin-puppeteer": "^3.0.0-rc.0",
        "@koishijs/plugin-status": "^5.0.0-rc.2",
        "@koishijs/plugin-switch": "file:.yalc/@koishijs/plugin-switch",
        "@koishijs/plugin-teach": "^3.0.0-rc.1",
        "axios": "^0.24.0",
        "canvas": "^2.8.0",
        "koishi": "^4.0.0-rc.3",
        "koishi-plugin-assets-smms": "^1.0.0-rc.1",
        "koishi-plugin-image-search": "^4.0.0-rc.0",
        "rss-feed-emitter": "^3.2.2",
        "textversionjs": "^1.1.3"
    }
}

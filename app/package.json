{
    "name": "oni-chat",
    "types": "dist/index.d.ts",
    "typings": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "version": "1.0.0",
    "license": "MIT",
    "repository": "https://github.com/ONI-Wiki-zh/ONIChat",
    "author": "DDElephant",
    "scripts": {
        "start": "koishi start ./src/koishi.config.ts -- -r ts-node/register/transpile-only",
        "start-random": "koishi start ./src/koishi.config.random.ts -- -r ts-node/register/transpile-only",
        "yallage": "cross-env NODE_ENV=production koishi start ./src/koishi.config.yallage.ts -- -r ts-node/register/transpile-only",
        "leapscloud": "cross-env NODE_ENV=production koishi start ./src/koishi.config.leapscloud.ts -- -r ts-node/register/transpile-only",
        "yallage-test": "koishi start ./src/koishi.config.yallage.ts -- -r ts-node/register/transpile-only",
        "lint": "eslint . --fix",
        "dev": "ts-node src index.ts --trace-warnings",
        "build": "tsc",
        "build-js": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true tsc",
        "run-build": "cross-env NODE_ENV=production node ./dist/index.js",
        "run-relay": "cross-env NODE_ENV=production node ./dist/relayBot.js",
        "test-relay": "cross-env NODE_ENV=production ts-node src/relayBot.ts --trace-warnings",
        "test": "mocha tests",
        "test-build": "node ./dist/index.js"
    },
    "devDependencies": {
        "@koishijs/cli": "^4.1.2",
        "@types/jquery": "^3.5.9",
        "@types/mocha": "^9.1.0",
        "@types/node": "^16.11.7",
        "@types/textversionjs": "^1.1.1",
        "@typescript-eslint/eslint-plugin": "^5.4.0",
        "@typescript-eslint/parser": "^5.4.0",
        "cross-env": "^7.0.3",
        "eslint": "^8.2.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "mocha": "^9.2.0",
        "prettier": "^2.4.1",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "@koishijs/plugin-adapter-discord": "^2.0.1",
        "@koishijs/plugin-adapter-onebot": "^4.0.1",
        "@koishijs/plugin-adapter-telegram": "^2.1.0",
        "@koishijs/plugin-admin": "^1.0.0",
        "@koishijs/plugin-assets-local": "^2.0.1",
        "@koishijs/plugin-bind": "^1.0.0",
        "@koishijs/plugin-callme": "^1.0.0",
        "@koishijs/plugin-chat": "^1.1.1",
        "@koishijs/plugin-console": "^2.1.0",
        "@koishijs/plugin-database-memory": "^1.0.0",
        "@koishijs/plugin-database-mysql": "^4.1.1",
        "@koishijs/plugin-echo": "^1.0.0",
        "@koishijs/plugin-feedback": "^1.0.3",
        "@koishijs/plugin-github": "^4.0.1",
        "@koishijs/plugin-logger": "^1.1.1",
        "@koishijs/plugin-manager": "^1.3.0",
        "@koishijs/plugin-puppeteer": "^3.0.0",
        "@koishijs/plugin-recall": "^1.0.1",
        "@koishijs/plugin-status": "^5.2.1",
        "@koishijs/plugin-sudo": "^1.0.0",
        "@koishijs/plugin-switch": "^1.0.0",
        "@koishijs/plugin-teach": "^3.1.1",
        "axios": "^0.24.0",
        "canvas": "^2.8.0",
        "koishi": "^4.1.2",
        "koishi-plugin-assets-smms": "^1.0.0-rc.1",
        "koishi-plugin-blive": "^1.0.1",
        "koishi-plugin-image-search": "^4.0.0",
        "markdown-it": "^12.3.2",
        "mineflayer": "^3.14.1",
        "mw.js": "^2.1.1",
        "prismarine-entity": "^2.0.0",
        "prismarine-item": "^1.11.1",
        "rss-feed-emitter": "^3.2.2",
        "rss-parser": "^3.12.0",
        "textversionjs": "^1.1.3"
